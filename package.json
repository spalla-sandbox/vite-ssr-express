    "date-fns": "^2.30.0",
    "express": "^4.18.2",
    "radix3": "^1.0.1",
    "sirv": "^2.0.3",
    "ufo": "^1.1.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/node": "^20.2.5",
    "cross-env": "^7.0.3",
    "typescript": "^5.1.3",
    "vite": "^4.3.9"
  }
}
